#include<stdio.h>
int main()
{
    int a, b, c;
    int max=0;
    printf("请输入三个数字：",a,b,c);
    scanf("%d,%d,%d",&a,&b,&c);
    if (a>b)
    {
       if(a>c){
        max=a;
       }
       else{
        max=c;
       }
    }
    printf("%d\n",max);
    return 0;
}
    






    
    

    
}

 